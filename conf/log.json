{
  "desc":"development",
  "level":"debug",
  "stdout":true,
  "encoding":"console",
  "addCaller":true,
  "color":true,
  "filesout":true,
  "logspath":[
    {
      "level":"debug",
      "hook":{
        "filename": "logs/app.log",
        "maxsize": 1,
        "maxage": 7,
        "maxbackups": 3,
        "localtime": true,
        "compress": true
      }
    }
  ]
}